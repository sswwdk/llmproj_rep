{
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                // 첫 번째 규칙: 마크다운 제목
                "scope": "markup.heading.markdown",
                "settings": {
                    "foreground": "#02fa9f" 
                }
            }, // <-- 첫 번째 규칙이 끝나고 쉼표(,)를 추가합니다.
            {
                // 두 번째 규칙: 주석
                "scope": "comment",
                "settings": {
                    "foreground": "#ff61b8" 
                }
            }
        ]
    }
}
